-- ì •ë‹µ
-- DB: codingtest / Table: hub_university
select name,
case
when absent >= 4 then 'F'
when score >= 90 then 'A'
when score >= 80 then 'B'
when score >= 70 then 'C'
when score >= 60 then 'D'
else 'F'
end as grade
from hub_university hu ;

------------------------------
create table a_company(
	idx int not null,
    title VARCHAR(255) NOT NULL,
    notice_dept VARCHAR(50) NOT NULL,
    post_date DATETIME NOT NULL,
    view_count INT NOT null,
    primary key (idx)
);

INSERT INTO a_company (idx, title, notice_dept, post_date, view_count) VALUES
(1, 'ê°œë°œíŒ€ ì „ì²´ ê³µì§€', 'ê°œë°œíŒ€', '2023-01-05 00:00:00', 205),
(2, 'ê¸°íšíŒ€ ì „ì²´ ê³µì§€', 'ê¸°íšíŒ€', '2023-01-20 00:00:00', 98),
(3, 'ë””ìì¸íŒ€ ê³µìœ  ì‚¬ì´íŠ¸ ê³µì§€', 'ë””ìì¸íŒ€', '2023-01-29 00:00:00', 150),
(4, 'íšŒê³„íŒ€ Q ì»¨í¼ëŸ°ìŠ¤ ì •ë¦¬ ê³µì§€', 'íšŒê²ŒíŒ€', '2023-02-05 00:00:00', 135),
(5, 'ê°œë°œíŒ€ Y í”„ë¡œì íŠ¸ ë¯¸íŒ… ê³µì§€', 'ê°œë°œíŒ€', '2023-02-06 00:00:00', 86),
(6, 'ë””ìì¸íŒ€ X í”„ë¡œì íŠ¸ ë¯¸íŒ… ê³µì§€', 'ë””ìì¸íŒ€', '2023-02-06 00:00:00', 52);


SELECT
ac.notice_dept,
ac.title,
ac.view_count,
ac.post_date
FROM a_company ac
JOIN (
SELECT notice_dept, MAX(post_date) AS max_post_date
FROM a_company
GROUP BY notice_dept
) l
on ac.notice_dept = l.notice_dept
and ac.post_date = l.max_post_date;
